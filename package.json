{
  "name": "@itemku/urlman",
  "version": "0.0.1",
  "description": "Redirect url from param",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FIVEJACK/urlman.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "prebuild": "rimraf ./dist",
    "build": "tsc && tsc-alias",
    "postbuild": "cp package.json dist/package.json && cd dist && yarn install --production",
    "bundle": "cd ./dist && bestzip build.zip *  && copyfiles build.zip ./../"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.18.10",
    "@types/aws-lambda": "8.10.56",
    "@types/node": "^18.6.5",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.33.0",
    "bestzip": "^2.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.6.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-standard": "^14.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.5.2"
  }
}
